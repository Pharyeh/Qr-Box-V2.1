{
  "name": "qr-box-client",
  "version": "2.1.0",
  "description": "QR Box v2.1 â€” Market Reflex Intelligence",
  "author": "PharyehNation",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "electron .",
    "build": "react-scripts build",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.qrbox.app",
    "productName": "QR Box",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "public/electron.js"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "chart.js": "^4.4.1",
    "framer-motion": "^12.12.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "electron": "^25.9.0",
    "electron-builder": "^24.13.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
