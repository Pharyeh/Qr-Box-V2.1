# QR Box Demo - Environment Variables
# Copy this file to server/.env and fill in your API keys

# Required: OpenAI API Key for AI features (GPT Thesis, Trade Ideas)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: OANDA API for enhanced forex data
OANDA_API_KEY=your_oanda_api_key_here
OANDA_ACCOUNT_ID=your_oanda_account_id_here

# Optional: Tingo API for additional market data
TINGO_API_KEY=your_tingo_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development 